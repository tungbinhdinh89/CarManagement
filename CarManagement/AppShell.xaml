<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="CarManagement.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:CarManagement.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="CarManagement">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Add Car"
        ContentTemplate="{DataTemplate local:AddCarPage}"
        Route="AddCarPage" />

</Shell>
